<div class="row">
  <div class="col s12 m8">
    <h5>{{cv.fullName}}</h5>
    <p>Current Location: {{cv.location}}</p>
    <p>Current Role: {{cv.currentRole}}</p>
    <p>Looking For: {{cv.desiredRole}}</p>
    <div class="card story">
      <div class="card-content">
        <span class="card-title">{{formatDate story.date 'MMMM Do YYYY'}}</span>
        {{{cv.description}}}
      </div>
    </div>
  </div>
  <div class="col s12 m4">
    <div class="card center-align">
      <div class="card-content">
        <span class="card-title">{{cv.fullName}}</span>
        <img src="{{cv.photo}}" class="circle responsive-img">
        <span class="card-title">{{formatDate story.date 'MMMM Do YYYY'}}</span>
      </div>
      {{!--
      <div class="card-action">
        <a href="/stories/user/{{story.user.id}}">More From {{story.user.firstName}}</a>
      </div> --}}
    </div>
  </div>
</div>